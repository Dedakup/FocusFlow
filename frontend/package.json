        "next": "^14.1.0",
        "nprogress": "^0.2.0",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "react-redux": "^9.1.2",
        "react-toastify": "^10.0.6",
        "react-youtube": "^10.1.0"
